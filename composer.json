{
  "name": "veiliglanceren/laravel-oop-config",
  "version": "0.0.1",
  "description": "Type hinting and autocomplete for config values",
  "type": "library",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
    "pestphp/pest": "^2.0 || ^3.0",
    "pestphp/pest-plugin-laravel": "^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "VeiligLanceren\\LaravelOopConfig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OopConfigServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
